\b0 // C program for implementation of Bubble sort \par
#include <stdio.h> \par
\par
// Swap function \par
void swap(int* arr, int i, int j) \par
\{ \par
\tab int temp = arr[i]; \par
\tab arr[i] = arr[j]; \par
\tab arr[j] = temp; \par
\} \par
\par
// A function to implement bubble sort \par
void bubbleSort(int arr[], int n) \par
\{ \par
\tab int i, j; \par
\tab for (i = 0; i < n - 1; i++) \par
\par
\tab\tab // Last i elements are already \par
\tab\tab // in place \par
\tab\tab for (j = 0; j < n - i - 1; j++) \par
\tab\tab if (arr[j] > arr[j + 1]) \par
\tab\tab swap(arr, j, j + 1); \par
\} \par
// Function to print an array \par
void printArray(int arr[], int size) \par
\{ \par
\tab int i; \par
\tab for (i = 0; i < size; i++) \par
\tab\tab printf("%d ", arr[i]); \par
\tab printf("\\n"); \par
\} \par
\par
// Driver code \par
int main() \par
\{ \par
\tab int arr[] = \{ 5, 1, 4, 2, 8 \}; \par
\tab int N = sizeof(arr) / sizeof(arr[0]); \par
\tab bubbleSort(arr, N); \par
\tab printf("Answer: "); \par
\tab printArray(arr, N); \par
\tab return 0; \par
\}\par
